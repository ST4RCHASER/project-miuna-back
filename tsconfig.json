{
  "compileOnSave": true,
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dlist",  
    "sourceMap": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true, 
    "strictPropertyInitialization": false,
    "strict": true,
    "baseUrl": ".",          
    "moduleResolution": "node",
    "paths": {
      "@project-miuna/utils": [
        "src/utils/index.ts",
      ],
    },
  },
}