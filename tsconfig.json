{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "outDir": "./dlist",
    "sourceMap": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2018",
    "module": "commonjs",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      "@project-miuna/utils": [
        "utils/index.ts"
      ],
    },
  },
}